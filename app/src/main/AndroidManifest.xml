<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lab3_3" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Lab3_3" >

        <activity
            android:name="com.example.lab3_3.MainActivity"
            android:exported="true"
            android:label="Activity 1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.lab3_3.Activity2"
            android:exported="true"
            android:parentActivityName=".MainActivity"
            android:label="Activity 2"/>
        <activity
            android:name="com.example.lab3_3.Activity3"
            android:exported="true"
            android:parentActivityName=".Activity2"
            android:label="Activity 3"/>
        <activity
            android:name="com.example.lab3_3.ActivityAbout"
            android:exported="true"
            android:parentActivityName=".MainActivity"
            android:label="About activity"/>
    </application>

</manifest>